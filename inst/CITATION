year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual", 
		 title = "{fuzzyreg}: Fuzzy Linear Regression",
		 author = c(person("Pavel", "Skrabanek"), person("Natalia", "Martinkova")),
		 year = year,
		 note = note,
		 url = "https://CRAN.R-project.org/package=fuzzyreg")
		 
